-- blood_types table
CREATE TABLE IF NOT EXISTS blood_types (
  blood_type_id INT PRIMARY KEY AUTO_INCREMENT,
  blood_group ENUM('A', 'B', 'AB', 'O') NOT NULL,
  rh_factor ENUM('positive', 'negative') NOT NULL
) COMMENT='Stores information about blood types';

-- hospitals table
CREATE TABLE IF NOT EXISTS hospitals (
  hospital_id INT PRIMARY KEY AUTO_INCREMENT,
  hospital_name VARCHAR(100) NOT NULL,
  address TEXT,
  phone_number VARCHAR(20) NOT NULL,
  email VARCHAR(100) NOT NULL,
  INDEX idx_hospitals_phone_number (phone_number),
  INDEX idx_hospitals_email (email)
) COMMENT='Stores information about hospitals';

-- donors table
CREATE TABLE IF NOT EXISTS donors (
  donor_id INT PRIMARY KEY AUTO_INCREMENT,
  full_name VARCHAR(100) NOT NULL,
  address TEXT,
  phone_number VARCHAR(20) NOT NULL,
  email VARCHAR(100) NOT NULL,
  blood_type_id INT NOT NULL,
  medical_history LONGTEXT,
  last_donation_date DATE,
  FOREIGN KEY (blood_type_id) REFERENCES blood_types(blood_type_id),
  INDEX idx_donors_blood_type_id (blood_type_id),
  INDEX idx_donors_phone_number (phone_number),
  INDEX idx_donors_email (email)
) COMMENT='Stores information about blood donors';

-- donations table
CREATE TABLE IF NOT EXISTS donations (
  donation_id INT PRIMARY KEY AUTO_INCREMENT,
  donor_id INT NOT NULL,
  donation_date DATE NOT NULL,
  quantity DECIMAL(10,2) NOT NULL,
  comments LONGTEXT,
  FOREIGN KEY (donor_id) REFERENCES donors(donor_id),
  INDEX idx_donations_donor_id (donor_id),
  INDEX idx_donations_donation_date (donation_date)
) COMMENT='Stores information about blood donations';

-- request_blood table
CREATE TABLE IF NOT EXISTS request_blood (
  request_id INT PRIMARY KEY AUTO_INCREMENT,
  hospital_id INT NOT NULL,
  blood_type_id INT NOT NULL,
  quantity DECIMAL(5,2) NOT NULL,
  urgency_level ENUM('low', 'medium', 'high') NOT NULL,
  request_date DATE NOT NULL,
  FOREIGN KEY (hospital_id) REFERENCES hospitals(hospital_id),
  FOREIGN KEY (blood_type_id) REFERENCES blood_types(blood_type_id),
  INDEX idx_request_blood_hospital_id_blood_type_id (hospital_id, blood_type_id),
  INDEX idx_request_blood_request_date (request_date)
) COMMENT='Stores information about blood requests from hospitals';


-- distributions table
CREATE TABLE IF NOT EXISTS distributions (
  distribution_id INT PRIMARY KEY AUTO_INCREMENT,
  request_id INT,
  hospital_id INT,
  blood_type_id INT,
  quantity DECIMAL(10,2) NOT NULL,
  distribution_date DATE,
  recipient_details LONGTEXT,
  FOREIGN KEY (request_id) REFERENCES request_blood(request_id),
  FOREIGN KEY (hospital_id) REFERENCES hospitals(hospital_id),
  FOREIGN KEY (blood_type_id) REFERENCES blood_types(blood_type_id),
  INDEX idx_distributions_request_id_hospital_id_blood_type_id (request_id, hospital_id, blood_type_id),
  INDEX idx_distributions_distribution_date (distribution_date)
) COMMENT='Stores information about blood distributions';

-- camping table
CREATE TABLE IF NOT EXISTS camping (
  camp_id INT PRIMARY KEY AUTO_INCREMENT,
  location VARCHAR(100) NOT NULL,
  date DATE NOT NULL,
  organizers VARCHAR(100),
  expected_donors INT,
  collected_blood_volume DECIMAL(15,2),
  hospital_id INT,
  FOREIGN KEY (hospital_id) REFERENCES hospitals(hospital_id),
  INDEX idx_camping_hospital_id (hospital_id),
  INDEX idx_camping_date (date)
) COMMENT='Stores information about blood donation camps';

-- announcements table
CREATE TABLE IF NOT EXISTS announcements (
  announcement_id INT PRIMARY KEY AUTO_INCREMENT,
  announcement_title VARCHAR(200) NOT NULL,
  announcement_date DATE NOT NULL,
  announcement_body MEDIUMTEXT,
  hospital_id INT,
  FOREIGN KEY (hospital_id) REFERENCES hospitals(hospital_id),
  INDEX idx_announcements_hospital_id (hospital_id),
  INDEX idx_announcements_announcement_date (announcement_date)
) COMMENT='Stores information about announcements';


-- Inserting values for each table

